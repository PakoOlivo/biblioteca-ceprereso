{
  "productName": "biblioteca-ceprereso",
  "version": "0.1.0",
  "identifier": "com.biblioteca.ceprereso.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "windows": [
      {
        "title": "biblioteca-ceprereso",
        "width": 800,
        "height": 600,
        "label": "main"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "sql": {
      "default": {
        "loadSqlite": true
      }
    }
  }
}
